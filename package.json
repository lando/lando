{
  "name": "lando",
  "description": "The best local development solution in the galaxy.",
  "license": "GPL-3.0",
  "version": "3.0.0-beta.36",
  "repository": {
    "type": "git",
    "url": "https://github.com/lando/lando"
  },
  "bugs": {
    "email": "mike@thinktandem.io",
    "url": "https://github.com/lando/lando/issues"
  },
  "main": "lib/lando.js",
  "bin": {
    "lando": "./bin/lando.js"
  },
  "directories": {
    "lib": "./lib"
  },
  "pkg": {
    "scripts": [
      "bin/*.js",
      "lib/**/*.js",
      "plugins/**/*.js"
    ],
    "assets": [
      "package.json",
      "**/fastcgi_params",
      "**/*.cnf",
      "**/*.conf",
      "**/*.ini",
      "**/*.php",
      "**/*.yml",
      "**/*.sh",
      "**/*.vcl",
      "**/*.json",
      "**/*.toml"
    ]
  },
  "dependencies": {
    "bluebird": "^3.4.1",
    "cli-table": "^0.3.1",
    "deasync": "^0.1.12",
    "dockerode": "^2.4.2",
    "dotenv": "^4.0.0",
    "fs-extra": "^0.18.2",
    "github": "^9.2.0",
    "inquirer": "^3.1.1",
    "ip": "^1.1.5",
    "js-yaml": "^3.4.6",
    "jsonfile": "^2.4.0",
    "kalabox-stats-client": "^0.2.1",
    "linus": "git+https://github.com/tomas/linus.git#v0.0.5",
    "localtunnel": "^1.8.3",
    "lodash": "^4.15.0",
    "node-cache": "^4.1.1",
    "object-hash": "^1.1.8",
    "restler": "^3.4.0",
    "semver": "^5.4.1",
    "shell-escape": "https://github.com/thinktandem/node-shell-escape/tarball/master",
    "shelljs": "^0.6.0",
    "sudo-block": "^2.0.0",
    "sync-exec": "^0.6.2",
    "uuid": "^2.0.1",
    "winston": "^2.2.0",
    "yargonaut": "^1.1.2",
    "yargs": "^6.6.0",
    "yargs-parser": "^4.2.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "copy": "^0.3.1",
    "eslint": "^4.15.0",
    "eslint-config-google": "^0.9.1",
    "gitbook-cli": "^2.3.0",
    "jsdoc-to-markdown": "^4.0.1",
    "mocha": "^4.1.0",
    "pkg": "^4.3.0-beta.5"
  },
  "scripts": {
    "docs": "gitbook install && gitbook build && ./scripts/docs.js",
    "lint": "eslint bin/**/*.js lib/**/*.js plugins/**/*.js test/**/*.js scripts/**/**/*.js",
    "release:major": "node ./scripts/bump.js --stage=major",
    "pkg:cli": "node ./scripts/pkg.js --stage=cli",
    "pkg:full": "node ./scripts/pkg.js --stage=full",
    "release:minor": "node ./scripts/bump.js --stage=minor",
    "release:patch": "node ./scripts/bump.js --stage=patch",
    "release:pre": "node ./scripts/bump.js --stage=prerelease",
    "serve": "gitbook install && ./scripts/docs.js && gitbook serve",
    "test:spec": "mocha",
    "test": "yarn lint && yarn test:spec"
  }
}
